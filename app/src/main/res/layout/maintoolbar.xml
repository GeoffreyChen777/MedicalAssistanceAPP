<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:background="@color/colorE2E4E3"
    android:orientation="horizontal">
    <ImageButton
        android:id="@+id/menuButton"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="7dp"
        android:background="@mipmap/ic_menu_black"
        android:scaleType="center"
        android:contentDescription="@null"
        android:layout_centerVertical="true"
        />
    <TextView
        android:id="@+id/titleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HEALTH ASSISTANT"
        android:layout_centerInParent="true"
        android:textStyle="bold"/>
    <ImageButton
        android:id="@+id/highFrequencyButton"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginRight="7dp"
        android:background="@mipmap/ic_start_black"
        android:scaleType="center"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:background="@color/color161D37"/>

</RelativeLayout>
